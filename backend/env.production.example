# Production Environment Variables for Render Deployment
# Copy this file to .env and fill in your actual values

# Database Configuration
MONGO_URI=mongodb+srv://username:password@cluster.mongodb.net/fit-tracker?retryWrites=true&w=majority

# JWT Configuration (must be at least 32 characters)
SECRET_KEY=your-super-secret-jwt-key-at-least-32-characters-long-for-production

# Frontend URL (your Vercel domain)
FRONTEND_URL=https://your-app-name.vercel.app

# Environment
NODE_ENV=production

# Server Configuration
PORT=4000

# Security
RATE_LIMIT_MAX=100
CORS_ORIGIN=https://your-app-name.vercel.app
